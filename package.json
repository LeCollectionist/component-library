{
  "name": "lc-component-library",
  "version": "1.2.6",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/lc-component-library.umd.js",
  "module": "./dist/lc-component-library.es.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "node build/exports.js && vite build && tsc --emitDeclarationOnly",
    "storybook": "start-storybook -p 6006",
    "dev": "vite",
    "build-storybook": "build-storybook"
  },
  "exports": {
    ".": {
      "import": "./dist/lc-component-library.es.js",
      "require": "./dist/lc-component-library.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "dependencies": {
    "@vee-validate/i18n": "^4.1.20",
    "@vee-validate/rules": "^4.1.20",
    "esno": "0.4.4",
    "vee-validate": "^4.2.4",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.4.3",
    "@babel/core": "^7.13.14",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.2.9",
    "@tailwindcss/postcss7-compat": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@vitejs/plugin-vue": "1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.7.1",
    "cross-env": "^7.0.3",
    "eslint": "7.23.0",
    "fast-glob": "^3.2.5",
    "postcss": ">=8.2.10",
    "tailwindcss": "2.1.0",
    "tsup": "^4.3.1",
    "typescript": "4.2.3",
    "vite": "2.1.5",
    "vue-loader": "^16.2.0"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "ignorePatterns": [
      "build/**/*.js"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
