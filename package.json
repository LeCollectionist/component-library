{
  "name": "lc-component-library",
  "version": "1.6.6",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/lc-component-library.umd.js",
  "module": "./dist/lc-component-library.es.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "vite build",
    "storybook": "start-storybook -p 6006",
    "dev": "vite",
    "build-storybook": "build-storybook",
    "test:unit": "jest --watch",
    "test:prod": "node ./node_modules/.bin/jest"
  },
  "exports": {
    ".": {
      "import": "./dist/lc-component-library.es.js",
      "require": "./dist/lc-component-library.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "dependencies": {
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "esno": "0.4.4",
    "vue": "3.2.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.4.3",
    "@babel/core": "^7.13.14",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.3.7",
    "@tailwindcss/postcss7-compat": "^2.1.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@vitejs/plugin-vue": "1.2.1",
    "@vue/test-utils": "^2.0.0-rc.6",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "chromatic": "5.9.0",
    "cross-env": "^7.0.3",
    "eslint": "7.23.0",
    "fast-glob": "^3.2.5",
    "jest": "^26.6.3",
    "postcss": ">=8.2.10",
    "tailwindcss": "2.1.0",
    "ts-jest": "^26.5.6",
    "tsup": "^4.3.1",
    "typescript": "4.2.3",
    "vite": "2.5.0",
    "vue-jest": "^5.0.0-alpha.9",
    "vue-loader": "^16.2.0",
    "vue-tsc": "0.2.3"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "ignorePatterns": [
      "build/**/*.js"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
